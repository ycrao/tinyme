{
  "name": "ycrao/tinyme",
  "description": "a tiny php framework based on flight and medoo.",
  "homepage": "https://github.com/ycrao/tinyme",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "raoyc",
      "email": "raoyc2009@gmail.com",
      "homepage": "https://raoyc.com/"
    }
  ],
  "require": {
    "php": ">=7.4",
    "flightphp/core": "^3.0",
    "catfan/medoo": "^2.1",
    "vlucas/phpdotenv": "^5.6",
    "monolog/monolog": "^3.0",
    "flightphp/cache": "^1.1",
    "ext-json": "*"
  },
  "config": {
    "preferred-install": "dist",
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "app\\": "app/"
    }
  },
  "scripts": {
    "start": "php -S 127.0.0.1:9999 -t public",
    "post-create-project-cmd": [
      "@php -r \"copy('.env.example', '.env');\""
    ]
  }
}